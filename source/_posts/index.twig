---
id: 1
layout: "_layouts/blog.twig"
title: "Celyes.dev - browse all articles"
relativeUrl: "index"
categories:
    - index
img: "#"
---
{% for post in posts|reverse %}
    {% if (post.id >= 10) %} 
    <div class="article-card">
        <h4 class="article-card-title"> 
            <a rel="noopener" href="{{site_url}}/{{post.relativeUrl}}" class="purple-text text-darken-4">
                {{post.title}} 
            </a>
        </h4> 
        {% include '_snippets/postinfo.twig' %}
        <span class="right">
            {% for category in post.categories %}
                <div class="chip">
                    {{ category }}
                </div>    
            {% endfor %}    
        </span>  
        <br><br>
        <div class="divider"></div>
    </div>
    {% endif %}
{% endfor %}